<Window x:Class="Rofl.UI.Main.Views.SingleReplayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rofl.UI.Main.Views"
        xmlns:controls="clr-namespace:Rofl.UI.Main.Controls"
        mc:Ignorable="d"
        Title="{StaticResource ProgramName}"
        MinHeight="655"
        Height="655" 
        MinWidth="780"
        Width="780"
        MaxWidth="780"
        Loaded="Window_Loaded">
    <Grid>
        <controls:ReplayDetailControl x:Name="DetailView"/>
    </Grid>
</Window>
